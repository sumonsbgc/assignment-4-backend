{
	"version": 2,
	"builds": [
		{
			"src": "api/index.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/api/index.js"
		}
	],
	"headers": [
		{
			"source": "/api/auth/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "no-store, no-cache, must-revalidate"
				}
			]
		}
	]
}
